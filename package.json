    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "jest": "^26.5.2",
    "ts-jest": "^26.4.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ]
  }
